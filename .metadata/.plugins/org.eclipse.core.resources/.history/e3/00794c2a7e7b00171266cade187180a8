import java.util.Scanner;

public class Toggle {

	public static void main(String[] args) {
		String s;
		Scanner scanner=new Scanner(System.in);
		System.out.println("Enter the String");
		s=scanner.nextLine();
		for(int i=0;i<s.length();i++){
			Character c=s.charAt(i);
			if(c.isUpperCase(c)){
				//c.toLowerCase(c);
				c=c.toLowerCase(c);
			System.out.print(c);
			}
		if(c.isLowerCase(c)){
			c=c.toUpperCase(c);
			System.out.print(c);
		}
		
		}

	}

}
